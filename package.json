{
    "name": "ts-todo-pages",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "clean": "rm -rf dist",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "build": "tsc && (pnpm run test:json || true) && node generate-test-results.js",
        "rebuild": "pnpm run clean && pnpm run build",
        "test": "jest",
        "test:json": "jest --json --outputFile=reports/test/test-results.json",
        "test:mutation": "stryker run",
        "install": "pnpm run lint && pnpm run rebuild",
        "dev": "pnpm run build && cd dist && python3 -m http.server 8000"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@stryker-mutator/core": "^9.1.1",
        "@stryker-mutator/jest-runner": "^9.1.1",
        "@stryker-mutator/typescript-checker": "^9.1.1",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "eslint": "^9.35.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    }
}